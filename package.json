{
  "name": "web-directory",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier . --write",
    "postinstall": "prisma generate",
    "docker:build:local": "docker compose -f ./docker-compose.local.yml up --build",
    "docker:local": "npm run build && docker compose -f ./docker-compose.local.yml up -d",
    "docker:build:production": "docker compose -f ./docker-compose.production.yml build",
    "docker:run:production": "docker compose -f ./docker-compose.production.yml up -d",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:seed:local": "dotenv -e .env.local -- node prisma/seed.js",
    "prisma:migrate:production": "dotenv -e .env.production -- npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:migrate:local": "dotenv -e .env.local -- npx prisma migrate dev --schema=./prisma/schema.prisma --skip-seed",
 "migrate:all": "npm run prisma:migrate:local && npm run prisma:seed:local"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@mantine/core": "^5.10.5",
    "@mantine/hooks": "^5.10.5",
    "@mantine/rte": "^5.10.5",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "dayjs": "^1.11.11",
    "eslint": "8.40.0",
    "eslint-config-next": "^12.1.0",
    "leaflet": "^1.9.4",
    "next": "^13.4.12",
    "next-auth": "^4.22.1",
    "next-cloudinary": "^4.15.0",
    "next-intl": "^3.14.0",
    "nodemailer": "^6.9.13",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.2.1",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.3",
    "react-select": "^5.7.4",
    "react-share": "^4.4.1",
    "sharp": "^0.32.1",
    "slugify": "^1.6.6",
    "swiper": "^9.3.2",
    "tailwind-merge": "^2.3.0",
    "world-countries": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.1",
    "dotenv-cli": "^7.4.2",
    "postcss": "^8.4.38",
    "postcss-reporter": "^7.1.0",
    "prettier": "3.2.5",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.4.3"
  }
}
